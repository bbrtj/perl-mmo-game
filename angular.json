{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "front",
	"projects": {
		"game": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "sass"
				},
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": "front/game",
			"sourceRoot": "front/game/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "public",
						"index": {
							"input": "front/game/src/index.html",
							"output": "../dist/play.html"
						},
						"main": "front/game/src/main.ts",
						"polyfills": "front/game/src/polyfills.ts",
						"tsConfig": "front/game/tsconfig.app.json",
						"inlineStyleLanguage": "sass",
						"assets": [
							"front/game/src/favicon.ico",
							"front/game/src/assets"
						],
						"styles": [
							"front/game/src/styles.sass"
						],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "front/game/src/environments/environment.ts",
									"with": "front/game/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "game:build:production"
						},
						"development": {
							"browserTarget": "game:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "game:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "front/game/src/test.ts",
						"polyfills": "front/game/src/polyfills.ts",
						"tsConfig": "front/game/tsconfig.spec.json",
						"karmaConfig": "front/game/karma.conf.js",
						"inlineStyleLanguage": "sass",
						"assets": [
							"front/game/src/favicon.ico",
							"front/game/src/assets"
						],
						"styles": [
							"front/game/src/styles.sass"
						],
						"scripts": []
					}
				}
			}
		}
	},
	"defaultProject": "game"
}
